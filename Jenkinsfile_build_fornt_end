pipeline{
    agent any
     environment {
        randomID = "111111"
    } 
    stages{
        stage("Build the code"){
            steps {  
                script{
                    def randomID = sh(script: 'uuid', returnStdout: true)
                    echo "${randomID}" >> index.html
                    cat index.html
                } 
            }
        }
    }
    post{
        always {  
            cleanWs()
        }
    }
}